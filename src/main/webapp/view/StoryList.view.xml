<mvc:View
	controllerName="sap.ui.demo.embeddedanalytics.controller.StoryList"
		xmlns:m="sap.m"
		xmlns="sap.f"
		xmlns:mvc="sap.ui.core.mvc">
	<m:Page
		title="Stories"
		showNavButton="true"
		navButtonPress="onNavBack">
		<m:content>
			<FlexibleColumnLayout id="flexibleColumnLayout" backgroundDesign="Solid">
				<beginColumnPages>
					<mvc:XMLView id="beginView" viewName="sap.ui.demo.embeddedanalytics.view.StoryListMaster" />
				</beginColumnPages>
				<midColumnPages>
					<mvc:XMLView id="detailView" viewName="sap.ui.demo.embeddedanalytics.view.Detail" />
				</midColumnPages>
			</FlexibleColumnLayout>  
		</m:content>	
	</m:Page>
</mvc:View>
